{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      },
      "env": {
        "EXPO_NO_DOTENV": "1"
      }
    },
    "production-jsc": {
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      },
      "env": {
        "EXPO_NO_DOTENV": "1"
      }
    }
  },
  "submit": {
    "production": {},
    "production-jsc": {}
  }
}
